{
  "jsPlugins": [
    "./plugins/enforce-kebab-case-file-names.js",
    "./plugins/enforce-query-key-constants.js"
  ],
  "ignorePatterns": [
    "scripts/migrate-project-entity-table.ts",
    "packages/sdk/src/observability/otel/*",
    "docs/*"
  ],
  "rules": {
    "unicorn/no-useless-fallback-in-spread": "off",
    "no-unused-expressions": "off",
    "no-explicit-any": "warn",
    "enforce-kebab-case-file-names/kebab-case": "warn",
    "ban-types": "warn",
    "enforce-query-key-constants/enforce-query-key-constants": "warn"
  }
}
